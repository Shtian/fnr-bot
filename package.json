    "ts-node-dev": "^1.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "babel-jest": "^27.0.6",
    "copyfiles": "^2.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "ts-jest": "^27.1.5",
    "typescript": "^4.9.5"
  },
  "husky": {
